{
  "manifest_version": 2,

  "name": "Remember Youtube time",
  "description": "Never lose the youtube position again.",
  "version": "0.0.1",
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/*"],
      "css": ["style.css"],
      "js": ["start.js"]
    }
  ],
 "web_accessible_resources": [
     "script.js"
 ]
}